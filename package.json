    "react-router-dom": "^4.1.1",
    "react-tap-event-plugin": "^2.0.1",
    "react-textarea-autosize": "^4.0.5",
    "reactfire": "^1.0.0",
    "webpack-combine-loaders": "^2.0.3",
    "webpack-dev-server": "^1.16.2"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-class-properties"
